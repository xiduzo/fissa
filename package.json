{
  "name": "fissa",
  "version": "2.7.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "icons": "react-native-vector-image generate",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "test": "jest",
    "cruise": "npx depcruise src --include-only \"^src\" --config --output-type dot | dot -T svg > graph/dependency-graph.svg",
    "cruise:html": "npx depcruise src --include-only \"^src\" --config --output-type dot | dot -T svg | tee graph/dependency-cruiser-dir-graph.svg | depcruise-wrap-stream-in-html > graph/dependency-graph.html",
    "cruise:ddot": "npx depcruise src --include-only \"^src\" --config --output-type ddot | dot -T svg -Grankdir=TD > graph/dependency-graph.svg",
    "cruise:archi": "npx depcruise src --include-only \"^src\" --config --output-type archi | dot -T svg -Grankdir=TD > graph/dependency-graph.svg"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "react-native"
  },
  "dependencies": {
    "@klarna/react-native-vector-drawable": "0.4.0",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/netinfo": "9.3.6",
    "@react-navigation/native": "6.0.14",
    "@react-navigation/native-stack": "6.9.2",
    "@react-navigation/stack": "6.3.5",
    "@taoqf/react-native-mqtt": "3.0.4",
    "lottie-react-native": "5.1.4",
    "luxon": "3.1.0",
    "react": "18.2.0",
    "react-native": "0.70.6",
    "react-native-app-auth": "6.4.3",
    "react-native-config": "1.4.11",
    "react-native-encrypted-storage": "4.0.3",
    "react-native-fast-image": "8.6.3",
    "react-native-gesture-handler": "2.8.0",
    "react-native-linear-gradient": "2.6.2",
    "react-native-onboarding-swiper": "1.2.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "3.18.2",
    "react-native-shared-element": "0.8.4",
    "react-native-toast-message": "2.1.5",
    "react-native-vector-image": "0.3.3",
    "react-navigation-shared-element": "3.1.3",
    "spotify-web-api-js": "1.5.2"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@babel/runtime": "7.20.1",
    "@react-native-community/eslint-config": "3.2.0",
    "@types/jest": "29.2.3",
    "@types/luxon": "^3.1.0",
    "@types/react-native": "0.70.6",
    "@types/react-native-onboarding-swiper": "1.1.4",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "babel-jest": "26.6.3",
    "babel-plugin-transform-inline-environment-variables": "0.4.4",
    "dependency-cruiser": "^12.0.1",
    "eslint": "8.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "26.6.3",
    "metro-react-native-babel-preset": "0.73.3",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  }
}
